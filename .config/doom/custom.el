(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(auto-save-list-file-prefix "/home/user/.config/doom/cache/autosave/")
 '(avy-all-windows t)
 '(bookmark-default-file "~/.config/doom/cache/bookmarks")
 '(persp-save-dir "/home/user/.confi/doom/cache/workspaces/")
 '(project-list-file "~/.config/doom/cache/projects")
 '(projectile-cache-file "/home/user/.config/doom/cache/projectile.cache")
 '(projectile-known-projects-file "/home/user/.config/doom/cache/projectile.projects")
 '(projectile-project-search-path '("/home/user/repos/"))
 '(recentf-save-file "/home/user/.config/doom/cache/recentf")
 '(savehist-file "/home/user/.config/doom/cache/savehist")
 '(transient-history-file "/home/user/.config/doom/cache/transient/history")
 '(transient-values-file "/home/user/.config/doom/cache/transient/values"))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(org-document-title ((t (:inherit outline-1 :height 1.4))))
 '(org-level-1 ((t (:inherit outline-1 :height 1.2))))
 '(org-level-2 ((t (:inherit outline-2 :height 1.0))))
 '(org-level-3 ((t (:inherit outline-3 :height 1.0))))
 '(org-level-4 ((t (:inherit outline-4 :height 1.0))))
 '(org-level-5 ((t (:inherit outline-5 :height 1.0)))))
